module Wrappers_tests

import Wrappers

def int_1() : bool {
    let i = new Int(12);

    (match i.get_value() with
        12 => true
        x => {
            println("\ti.get_value != 12!");
            false
        }
    ) and (match i.get_hash() with
        12 => true
        x => {
            println("\ti.get_hash != 12!");
            false
        }
    )
}

